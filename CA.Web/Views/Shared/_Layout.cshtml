<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>CakeAwesome - @ViewBag.Title</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <meta name="viewport" content="width=device-width" />
    @Styles.Render("~/Content/css", "~/Content/themes/base/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <header>
        <div>
        </div>
    </header>
    <section>
        @RenderBody()
    </section>
    <footer>
    </footer>

    @Scripts.Render("~/bundles/jquery", "~/bundles/jqueryui", "~/bundles/colorpick", "~/bundles/raphael")
    @RenderSection("scripts", required: false)
</body>
</html>
